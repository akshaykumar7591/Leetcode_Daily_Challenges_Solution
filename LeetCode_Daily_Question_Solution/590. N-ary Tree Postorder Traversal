/*
// Definition for a Node.
class Node {
    public int val;
    public List<Node> children;

    public Node() {}

    public Node(int _val) {
        val = _val;
    }

    public Node(int _val, List<Node> _children) {
        val = _val;
        children = _children;
    }
};
*/

class Solution {
    public static void path(Node root,List<Integer> ans){
        if(root==null)return;
       for(int i=0;i<root.children.size();i++){
        path(root.children.get(i),ans);
       }
        ans.add(root.val);
    }
    public List<Integer> postorder(Node root) {
        List<Integer> ans=new ArrayList<>();
        path(root,ans);
        return ans;
        
    }
}
