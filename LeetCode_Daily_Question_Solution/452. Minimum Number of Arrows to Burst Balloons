class Solution {
    public int findMinArrowShots(int[][] points) {
        Arrays.sort(points,(a,b)->Integer.compare(a[1],b[1]));
        int count=1;
         System.out.print(points[0][1]);
        int end=points[0][1];
        for(int i=1;i<points.length;i++){
            System.out.println(points[i][0]);
            if(points[i][0]>end){
               count++;
               end=points[i][1];
            }

        }

        return count;
    }
}
